<InvoiceRegisters>
  <Invoices>
    <Payable>
      <InvoiceNumber from="invoice_id"></InvoiceNumber>
      <InvoiceDate from="date_issue"></InvoiceDate>
      <DueDate from="date_due"></DueDate>
      <TotalAmount>2706.00</TotalAmount>
      <Notes/>
      <Iban from="iban"></Iban>
      <Amount from="amount_total_tax"></Amount>
      <Currency from="currency"></Currency>
      <Vendor from="sender_name"></Vendor>
      <VendorAddress from="sender_address"></VendorAddress>
      <Details item="tuple">
        <Detail>
          <Amount from="item_amount_total"></Amount>
          <AccountId/>
          <Quantity from="item_quantity"></Quantity>
          <Notes from="item_description"></Notes>
        </Detail>
      </Details>
    </Payable>
  </Invoices>
</InvoiceRegisters>
